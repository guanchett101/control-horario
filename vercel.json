{
  "version": 2,
  "buildCommand": "npm install --prefix frontend && npm run build --prefix frontend",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install --prefix frontend",
  "framework": null,
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/empleados/(.*)",
      "dest": "/api/empleados.js"
    },
    {
      "src": "/api/empleados",
      "dest": "/api/empleados.js"
    },
    {
      "src": "/api/registros/(.*)",
      "dest": "/api/registros.js"
    },
    {
      "src": "/api/registros",
      "dest": "/api/registros.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
