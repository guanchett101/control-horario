{
  "version": 2,
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/auth",
      "destination": "/api/auth"
    },
    {
      "source": "/api/empleados",
      "destination": "/api/empleados"
    },
    {
      "source": "/api/registros",
      "destination": "/api/registros"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}