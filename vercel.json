{
  "buildCommand": "CI=false DISABLE_ESLINT_PLUGIN=true npm install && npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true"
  }
}
