{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/empleados/(.*)",
      "dest": "/api/empleados.js"
    },
    {
      "src": "/api/empleados",
      "dest": "/api/empleados.js"
    },
    {
      "src": "/api/registros/(.*)",
      "dest": "/api/registros.js"
    },
    {
      "src": "/api/registros",
      "dest": "/api/registros.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
